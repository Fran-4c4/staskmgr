    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run tmgr ",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Start tmgr module",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/python",
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal",
            "args": [
                "-m","tmgr","config_file","..config/appconfig.json"
            ],
            "justMyCode": false
        },
        {
            "name": "Start tmgr module2",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tmgr/__main__.py",  // Ejecuta el m√≥dulo principal
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal",
            "args": [
                "config_file", "../config/appconfig.json"  // Argumentos personalizados
            ],
            "justMyCode": false
        }
        
    ]
}